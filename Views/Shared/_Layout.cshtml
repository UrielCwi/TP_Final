<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - Topodiario - Apellido</title>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    </head>
    <body>
    <nav class="navbar bg-body-tertiary">
    <div class="container">
    <a class="navbar-brand" href="">
      <img src="\imagenes\image.webp" alt="Bootstrap" width="90" height="72">
    </a>
    <a class="navbar-brand" href="~/Home/Home.cshtml">
      <img src="\imagenes\Home.jpg" alt="Bootstrap" width="90" height="72">
    </a>
    <a class="navbar-brand" href="~/Home/Tareas.cshtml">
      <img src="\imagenes\Reloj.jpg" alt="Bootstrap" width="90" height="72">
    </a>
    <a class="navbar-brand" href="~/Home/Calendario.cshtml">
      <img src="\imagenes\Calendario.jpg" alt="Bootstrap" width="90" height="72">
    </a>
    <a class="navbar-brand" href='@Url.Action("Notificaciones","Home", new{IdUsuario = ViewBag.Usuario.IdUsuario})'>
      <img src="\imagenes\Notificaciones.jpg" alt="Bootstrap" width="90" height="72">
    </a>
    </div>
    </nav>
        @RenderBody()
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
